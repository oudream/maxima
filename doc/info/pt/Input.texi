@deffn {Função} save (@var{filename}, @var{nome_1}, @var{nome_2}, @var{nome_3}, ...)
@deffnx {Função} save (@var{filename}, values, functions, labels, ...)
@deffnx {Função} save (@var{filename}, [@var{m}, @var{n}])
